{
  "multipart": [
    {
      "apply": {
        "model": "fallout_wastelands:block/wastelandglass_2_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "fallout_wastelands:block/wastelandglass_2_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "fallout_wastelands:block/wastelandglass_2_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "fallout_wastelands:block/wastelandglass_2_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "fallout_wastelands:block/wastelandglass_2_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "fallout_wastelands:block/wastelandglass_2_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "fallout_wastelands:block/wastelandglass_2_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "fallout_wastelands:block/wastelandglass_2_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "fallout_wastelands:block/wastelandglass_2_noside",
        "y": 270
      }
    }
  ]
}